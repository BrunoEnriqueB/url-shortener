{
  "name": "url-shortener",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/src/index.js",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier . --write",
    "prepare": "husky",
    "test": "jest --setupFiles dotenv/config",
    "test:dev": "jest --setupFiles dotenv/config --watchAll",
    "dev:docker": "tsx watch --inspect=0.0.0.0 src/index.ts",
    "make:migrate": "node -r dotenv/config --loader ts-node/esm ./node_modules/.bin/knex migrate:make --knexfile=src/knexfile.ts -x ts --env development",
    "migrate": "node -r dotenv/config --loader ts-node/esm ./node_modules/.bin/knex migrate:latest --knexfile=src/knexfile.ts --env development",
    "rollback": "node -r dotenv/config --loader ts-node/esm ./node_modules/.bin/knex migrate:rollback --knexfile=src/knexfile.ts --env development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "objection": "^3.1.5",
    "pg": "^8.13.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths-jest": "^0.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "lint-staged": {
    "*": "prettier . --write"
  }
}
